// Advanced Payjar Program - Variables and Control Flow
public class main(@self){
var x = 10;
var y = 20;
var sum = x + y;
println(`X =${x}`);
println(`Y=${y}`);
println(`Sum=${sum}`);

// Conditional example
if sum > 25 {
    println("Sum is greater than 25!");
} else {
    println("Sum is 25 or less");
}

// Loop example
var i = 0;
while i < 5 {
    println(`Iteration:${i}`);
    i = i + 1;
}
println("Done!");
}